type Query {
    bookById(id: ID): Book
    shortUrlById(id: ID): ShortUrl
    getAllShortUrls: [ShortUrl]
}

type Book {
    id: ID
    name: String
    pageCount: Int
    author: Author
}

type Author {
    id: ID
    firstName: String
    lastName: String
}

type ShortUrl {
    id: ID
    shortenedUrl: String
    normalUrl: String
}